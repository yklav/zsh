# directory movement
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias 'bk=cd $OLDPWD'

# Colorize output, add file type indicator, and put sizes in human readable format
alias ls='ls -GFhl'

# Same as above, but in long listing format
alias ll='ls -GFhla'

# Git
alias gcl='git clone'
